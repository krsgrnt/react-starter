{
  "extends": ["plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "tabWidth": 4,
        "semi": true,
        "jsxBracketSameLine": true
      }
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}
